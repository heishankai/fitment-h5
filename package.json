{
  "name": "fitment-h5",
  "description": "装修H5项目工程",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.19.5",
    "pnpm": "7.22.0"
  },
  "scripts": {
    "start:dev": "vite",
    "start:test": "vite --mode test",
    "start:qa": "vite --mode qa",
    "build:dev": "run-p type-check \"build-only {@}\" --",
    "build:test": "run-p type-check \"build-only {@}\" --",
    "build:qa": "run-p type-check \"build-only {@}\" --",
    "build:prod": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky"
  },
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.2",
    "echarts": "^5.5.0",
    "js-cookie": "^3.0.5",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "platform": "^1.3.6",
    "postcss": "^8.0.0",
    "postcss-mobile-forever": "^4.1.4",
    "qs": "^6.12.1",
    "vant": "^4.9.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.7",
    "@types/lodash-es": "^4.17.12",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.12.5",
    "@types/platform": "^1.3.6",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.1.7",
    "lint-staged": "15",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "terser": "^5.31.1",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vite-plugin-style-import": "1.4.1",
    "vue-tsc": "^2.0.11"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
